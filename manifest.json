{
  "manifest_version": 3,
  "name": "Tab Discarder",
  "description": "Manually discard any inactive tab by title",
  "version": "1.0",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
